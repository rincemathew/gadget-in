<script>
  const deleteBanner = async (id) => {
            // console.log(`/categories/toggle/${id}/`)
            const response = await fetch(`/coupon/delete/${id}/`, { method: 'POST' });
            const resBody = await response.json();

            // if (!resBody.isOk) {
            //     console.log('Some problem occurred. Please reload the page');
            //     alert('Some problem occurred. Please reload the page');
            //     return;
            // }
            // Return if everything is ok
            if(resBody.message == 'coupon deleted successfully') {
                document.getElementById(id).style.display = 'none';
            }
            alertMsg(resBody.message)
            return;
        }

  function alertMsg(msg) {
        Swal.fire({
            icon: 'success',
            title: msg,
            showConfirmButton: false,
            timer: 1500
        })
    }
    
</script>


